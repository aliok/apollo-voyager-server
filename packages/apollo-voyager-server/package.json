{
  "name": "@aerogear/apollo-voyager-server",
  "version": "1.0.0",
  "description": "> TODO: description",
  "author": "AeroGear Team<aerogear@googlegroups.com>",
  "homepage": "http://aerogear.org",
  "license": "Apache-2.0",
  "main": "dist/apollo-voyager-server.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile": "tsc --build tsconfig.json",
    "watch": "tsc --build tsconfig.json --watch",
    "compile:clean": "tsc --build tsconfig.json --clean",
    "test": "ava 'src/*.test.ts' 'src/**/*.test.ts'",
    "integration_test": "ava --concurrency=1 'integration_test/*.test.ts' 'integration_test/**/*.test.ts'",
    "keycloak": "docker-compose -f keycloak/docker-compose.yml -p sync up"
  },
  "dependencies": {
    "@aerogear/apollo-voyager-audit": "^1.0.0",
    "@aerogear/apollo-voyager-context": "^1.0.0",
    "@aerogear/apollo-voyager-metrics": "^1.0.0",
    "@aerogear/apollo-voyager-tools": "^1.0.0",
    "apollo-server": "^2.2.2",
    "apollo-server-express": "^2.2.2"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.1.4",
    "apollo-cache-inmemory": "^1.3.0-beta.6",
    "apollo-client": "^2.3.7",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.8",
    "apollo-utilities": "^1.0.17",
    "ava": "1.0.0-rc.2",
    "node-fetch": "^2.2.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6",
    "ws": "^6.0.0"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.2"
  }
}
